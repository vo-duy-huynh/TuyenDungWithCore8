@model List<CompanyComment>
@{
    ViewBag.Title = "Quản lý Bình Luận";
}
<div class="content-header pt-5">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0">@ViewBag.Title</h1>
            </div><!-- /.col -->
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="#">Home</a></li>
                    <li class="breadcrumb-item active">@ViewBag.Title</li>
                </ol>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div><!-- /.container-fluid -->
</div>
<div class="card shadow border-0 my-4">
    <div class="card-body p-4">
        <div class="row pb-3">
            <div class="col-6">
            </div>
        </div>

        <table id="tblDataCompanyComment" class="table table-bordered table-striped" style="width:100%">
            <thead>
                <tr>
                    <th>Tên người dùng</th>
                    <th>Công ty</th>
                    <th>Bình luận</th>
                    <th>Đánh giá</th>
                    <th>Thời gian bình luận</th>
                    <th></th>
                </tr>
            </thead>


        </table>

    </div>
</div>

@section Scripts{
    <script src="~/js/companycomment1.js"></script>
}